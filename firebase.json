{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/load_map_on_request",
        "function": {
          "functionId" : "load_map_on_request",
          "region": "us-central1"
        }
      }
    ]
  },
  "functions": [
    {
      "codebase": "default",
      "region": "us-central1",
      "source": "functions",
      "runtime": "python313",
      "ignore": [
        "venv",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ]
    }
  ]
}
